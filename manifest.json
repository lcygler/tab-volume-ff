{
  "manifest_version": 2,
  "name": "Tab Volume Controller",
  "version": "1.0",
  "description": "Control volume for tabs playing sound.",
  "permissions": ["tabs", "<all_urls>", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
